<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.demo.auth.center.mapper.UserMapper">

    <select id="findByUsername" resultType="com.demo.auth.center.entity.model.SysUser">
            select * from sys_user where username = #{username}
    </select>

</mapper>